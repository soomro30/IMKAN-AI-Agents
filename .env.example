# Optional: Anthropic API key (not currently used by agents)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# UAE Pass Authentication
# Mobile number for UAE Pass login (format: +971XXXXXXXXX)
TAMM_MOBILE_NUMBER=your_mobile_number_here
